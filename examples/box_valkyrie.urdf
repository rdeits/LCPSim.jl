<?xml version="1.0"?>

<robot xmlns="http://drake.mit.edu"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://drake.mit.edu ../../doc/drakeURDF.xsd" name="BoxValkyrie">
  <link name="core">
    <inertial>
      <mass value="1.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="1.0" iyy="1.0" izz="1.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.2 0.05 0.4"/>
      </geometry>
    </visual>
  </link>

  <link name="rh_dummy">
    <inertial>
      <mass value="0.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>

  <link name="rh">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="1.0" iyy="1.0" izz="1.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
  </link>

  <joint name="core_to_rh_x" type="prismatic">
    <parent link="core"/>
    <child link="rh_dummy"/>
    <origin xyz="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="0.25" upper="0.75" velocity="10" effort="10"/>
  </joint>

  <joint name="core_to_rh_z" type="prismatic">
    <parent link="rh_dummy"/>
    <child link="rh"/>
    <origin xyz="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.5" upper="0.5" velocity="10" effort="10"/>
  </joint>

  <link name="lh_dummy">
    <inertial>
      <mass value="0.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>

  <link name="lh">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="1.0" iyy="1.0" izz="1.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
  </link>

  <joint name="core_to_lh_x" type="prismatic">
    <parent link="core"/>
    <child link="lh_dummy"/>
    <origin xyz="0 0 0"/>
    <axis xyz="-1 0 0"/>
    <limit lower="0.25" upper="0.75" velocity="10" effort="10"/>
  </joint>

  <joint name="core_to_lh_z" type="prismatic">
    <parent link="lh_dummy"/>
    <child link="lh"/>
    <origin xyz="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-0.5" upper="0.5" velocity="10" effort="10"/>
  </joint>

  <link name="rf_dummy">
    <inertial>
      <mass value="0.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>

  <link name="rf">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="1.0" iyy="1.0" izz="1.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
  </link>

  <joint name="core_to_rf_x" type="prismatic">
    <parent link="core"/>
    <child link="rf_dummy"/>
    <origin xyz="0 0 0"/>
    <axis xyz="1 0 0"/>
    <limit lower="0.05" upper="0.5" velocity="10" effort="10"/>
  </joint>

  <joint name="core_to_rf_z" type="prismatic">
    <parent link="rf_dummy"/>
    <child link="rf"/>
    <origin xyz="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1" upper="-0.55" velocity="10" effort="20"/>
  </joint>

    <link name="lf_dummy">
    <inertial>
      <mass value="0.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
  </link>

  <link name="lf">
    <inertial>
      <mass value="0.1"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="1.0" iyy="1.0" izz="1.0" ixy="0.0" ixz="0.0" iyz="0.0"/>
    </inertial>
    <visual>
      <geometry>
        <sphere radius="0.05"/>
      </geometry>
    </visual>
  </link>

  <joint name="core_to_lf_x" type="prismatic">
    <parent link="core"/>
    <child link="lf_dummy"/>
    <origin xyz="0 0 0"/>
    <axis xyz="-1 0 0"/>
    <limit lower="0.05" upper="0.5" velocity="10" effort="10"/>
  </joint>

  <joint name="core_to_lf_z" type="prismatic">
    <parent link="lf_dummy"/>
    <child link="lf"/>
    <origin xyz="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-1" upper="-0.55" velocity="10" effort="20"/>
  </joint>


</robot>


